{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Codex Settings Code Review Data Payload",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "source": {
      "type": "string"
    },
    "route": {
      "type": "string"
    },
    "http_status_code": {
      "type": "integer"
    },
    "http_status_reason": {
      "type": "string"
    },
    "redirect": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "target": {
          "type": "string"
        },
        "status": {
          "type": "integer"
        },
        "revalidate": {
          "type": "boolean"
        },
        "reload_behavior": {
          "type": "string"
        }
      },
      "required": [
        "target",
        "status",
        "revalidate",
        "reload_behavior"
      ]
    },
    "trace": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "kind": {
          "type": "string"
        },
        "stage": {
          "type": "integer"
        },
        "flags": {
          "type": "object",
          "additionalProperties": {
            "type": "integer"
          }
        }
      },
      "required": [
        "kind",
        "stage",
        "flags"
      ]
    }
  },
  "required": [
    "source",
    "route",
    "http_status_code",
    "http_status_reason",
    "redirect",
    "trace"
  ]
}
